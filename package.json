{
  "name": "funktx",
  "version": "0.1.0",
  "description": "helper functions, basic types, and essentials; FP style",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "browser": "dist/bundle.umd.js",
  "repository": "git@github.com:shahabkhalvati/funktx.git",
  "author": "Shahab Khalvati <contact@shahabkhalvati.com>",
  "license": "MIT",
  "keywords": [
    "template",
    "node",
    "module"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "watch 'npm run build' src",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "coverage": "npm run test -- --coverage",
    "coverage:ci": "npm run test -- --coverage --coverageReporters=text-lcov | coveralls",
    "prepublishOnly": "npm run build",
    "release": "np"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "babel-jest": "^27.3.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.2.0",
    "fast-check": "^2.19.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "np": "^7.5.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "rollup": "^2.60.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "watch": "^1.0.2"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
